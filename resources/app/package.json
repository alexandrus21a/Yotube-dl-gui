{
  "name": "Youtubify",
  "version": "1.0.1",
  "description": "A youtube-dl GUI downloader",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . live-download --overwrite --asar=false --platform=win32 --arch=ia32 --icon=assets/icons/win/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Live-Download\""
  },
  "build": {
    "appId": "com.live.download.live-download",
    "productionName": "Live-Download"
  },
  "keywords": [
    "youtube-dl",
    "mp3",
    "mp4",
    "downloader",
    "electron",
    "npm",
    "html"
  ],
  "author": "NukeZ",
  "license": "MIT",
  "dependencies": {
    "electron": "^7.2.3",
    "electron-installer-windows": "^3.0.0",
    "electron-simple-updater": "^2.0.9",
    "electron-wix-msi": "^3.0.6",
    "node-cmd": "^3.0.0"
  },
  "devDependencies": {
    "electron-packager": "^14.2.1"
  }
}
